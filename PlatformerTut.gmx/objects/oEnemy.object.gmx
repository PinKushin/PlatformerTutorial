            <string>Hsp = Dir * MoveSpeed
Vsp += Grav

//Horizontal Collision
if place_meeting (x + Hsp, y, oWall)
{
    while !place_meeting (x + sign(Hsp), y, oWall)
    {
        x += sign(Hsp)
    }
    Hsp = 0
    
    Dir *= -1
}
x += Hsp

//Vertical Collision
if place_meeting (x, y + Vsp, oWall)
{
    while !place_meeting (x, y + sign(Vsp), oWall)
    {
        y += sign(Vsp)
    }
    Vsp = 0
    if FearOfHeights and not position_meeting (x + (sprite_width/2) * Dir, y + (sprite_height/2) + FearOfHeightsLimit, oWall)
    {
        Dir *= -1
    }

}
y += Vsp




//Enemy Collision
if place_meeting (x,y, oPlayer)
{
    if oPlayer.y &lt; y - 16 //16px
    {
        with oPlayer
        {
            Vsp = -JumpSpeed
        }
        instance_destroy ()
    }
    else
    {
        scrDeath ()
    }
}
</string>
