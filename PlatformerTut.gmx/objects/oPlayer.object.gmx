            <string>
//Get Player Input

KeyRight = keyboard_check (ord ("D"))
KeyLeft = -keyboard_check (ord ("A"))
KeyJump = keyboard_check_pressed (vk_space)
KeyDown = keyboard_check_pressed (ord ("S"))

//React to Inputs
Move = KeyLeft + KeyRight
Hsp = Move * MoveSpeed

//Gravity
if (Vsp &lt; 10)
{
    Vsp += Grav
}

//Jump
if place_meeting (x, y + 1, oWall)
{
    Jumps = JumpsMax
}

if KeyJump and (Jumps &gt; 0)
{
    Jumps -= 1
    Vsp = -JumpSpeed
}

if (KeyJump) &amp;&amp; (place_meeting (x + 1, y, oWall) || place_meeting (x - 1, y, oWall))
{
    Vsp = -JumpSpeed
}
    
var HspFinal = Hsp + HspCarry
HspCarry = 0
//Horizontal Collision
if place_meeting (x + HspFinal, y, oWall)
{
    while !place_meeting (x + sign(HspFinal), y, oWall)
    {
        x += sign(HspFinal)
    }
    HspFinal = 0
    Hsp = 0
}
x += HspFinal

//Vertical Collision
if place_meeting (x, y + Vsp, oWall)
{
    while !place_meeting (x, y + sign(Vsp), oWall)
    {
        y += sign(Vsp)
    }
    Vsp = 0
}
y += Vsp

//Animation
if (Move != 0) image_xscale = Move










</string>
